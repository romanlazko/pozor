{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "akuechler/laravel-geoly": "^1.0",
        "anhskohbo/no-captcha": "^3.5",
        "deeplcom/deepl-php": "^1.7",
        "ezyang/htmlpurifier": "^4.17",
        "filament/forms": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/tables": "^3.2",
        "guava/filament-clusters": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "igaster/laravel_cities": "^1.3",
        "kirschbaum-development/eloquent-power-joins": "^3.5",
        "laravel/breeze": "^1.28",
        "laravel/framework": "^10",
        "laravel/sanctum": "^3.3",
        "laravel/scout": "^10.8",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "laravolt/avatar": "^5.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^3.5",
        "lukeraymonddowning/honey": "^1.0",
        "orangehill/iseed": "^3.0",
        "owen-it/laravel-auditing": "^13.6",
        "ralphjsmit/laravel-seo": "^1.5",
        "romanlazko/slurp": "^0.0.5",
        "romanlazko/telegram": "^1.0",
        "spatie/laravel-backup": "^8.5",
        "spatie/laravel-markdown": "^2.5",
        "spatie/laravel-medialibrary": "^11.4",
        "spatie/laravel-permission": "^6.3",
        "spatie/laravel-sluggable": "^3.5",
        "staudenmeir/eloquent-json-relations": "^1.1",
        "stevebauman/location": "^7.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
